CMD:get(playerid, params[])
{
    new x_job[30];
    if(sscanf(params,"s[30]",x_job)) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /get (fuel/guns/paintball)");
	if(strcmp(x_job,"guns",true) == 0)
	{
	    if(!IsATerra(playerid)) return	SendClientMessage(playerid, COLOR_GRAD1, "Вы не на своей базе");
		if(PlayerInfo[playerid][pMember] == 13 && PlayerInfo[playerid][pRank] >= 4 || PlayerInfo[playerid][pMember] == 12 && PlayerInfo[playerid][pRank] >= 4 || PlayerInfo[playerid][pMember] == 15 && PlayerInfo[playerid][pRank] >= 4 || PlayerInfo[playerid][pMember] == 17 && PlayerInfo[playerid][pRank] >= 4 || PlayerInfo[playerid][pMember] == 18 && PlayerInfo[playerid][pRank] >= 4)
		{
            switch(PlayerInfo[playerid][pMember])
			{
			    case 15: if(FracBank[0][fLock][0] == 1) return SendClientMessage(playerid, COLOR_GRAD1, "Склад вашей банды закрыт.");
				case 12: if(FracBank[0][fLock][1] == 1) return SendClientMessage(playerid, COLOR_GRAD1, "Склад вашей банды закрыт.");
				case 13: if(FracBank[0][fLock][2] == 1) return SendClientMessage(playerid, COLOR_GRAD1, "Склад вашей банды закрыт.");
				case 17: if(FracBank[0][fLock][3] == 1) return SendClientMessage(playerid, COLOR_GRAD1, "Склад вашей банды закрыт.");
				case 18: if(FracBank[0][fLock][4] == 1) return SendClientMessage(playerid, COLOR_GRAD1, "Склад вашей банды закрыт.");
			}
		    SPD(playerid, 9599, 2, "Склад оружия","> Взять личные материалы\n> Положить\n<> {6495ED}Оружие\tПатроны\tМатериалы\nSd Pistol\t100\t\t100\nDeagle\t\t100\t\t300\nShotgun\t100\t\t300\nSMG\t\t100\t\t200\nAK-47\t\t100\t\t300\nM4\t\t100\t\t300\nRifle\t\t100\t\t500", "Выбрать", "Отмена");
			return true;
		}
	}
	else if(strcmp(x_job, "fuel",true) == 0)
	{
	    new null;
	    for(new b = 1; b <= TOTALBIZZ; b++)
	    {
	        if(!IsPlayerInRangeOfPoint(playerid, 10.0, BizzInfo[b][bEntrx], BizzInfo[b][bEntry], BizzInfo[b][bEntrz]) || BizzInfo[b][bType] != 2) continue;
			new fuel = BizzInfo[b][bTill] / 3;
   			if(BizzInfo[b][bProduct] < 20 || BizzInfo[b][bLock] == 1) return SendClientMessage(playerid,COLOR_GRAD1,"На заправке не хватает топлива!");
			if(PlayerInfo[playerid][pFuel] > 3) return SendClientMessage(playerid,COLOR_GREY,"У вас уже есть 3 канистры.");
			if(PlayerInfo[playerid][pCash] < fuel * 20) return SendClientMessage(playerid,COLOR_GRAD1, "Не достаточно денег!");
			PlayerInfo[playerid][pCash] -= fuel * 20;
			BizzPay[b] += fuel * 20;
			BizzInfo[b][bMoney] += fuel * 20;
			BizzInfo[b][bProduct] -= 20;
			PlayerInfo[playerid][pFuel]++;
			SendMes(playerid,0x6495EDFF,"Вы купили канистру с 20 литрами бензина за %i вирт",fuel * 20);
			null++;
			break;
	    }
	    if(null == 0) return SendClientMessage(playerid, COLOR_GREY, "Вы должны находится у заправки!");
    }
	else if(strcmp(x_job, "paintball", true) == 0)
	{
	    if(PaintList)
	    {
	        new winner;
	        stringer[0] = 0;
	        strcat(stringer,"{0004FF}Команда синих:\n");
	        for(new i = 0; i < PaintList; i++) if(PaintInfo[i][Type] == 1) format(stringer1,sizeof(stringer1),"\n%s\t\tУбийств: %i\t\tСмертей: %i",PaintInfo[i][Name],PaintInfo[i][Kills],PaintInfo[i][Deaths]), strcat(stringer,stringer1), winner+=PaintInfo[i][Kills];
	        format(stringer1,sizeof(stringer1),"\n___________________________________________\nОбщее количество убийств: %i\n\n",winner), strcat(stringer,stringer1);
	        winner = 0;
	        strcat(stringer,"{FF0000}Команда красных:\n");
	        for(new i = 0; i < PaintList; i++) if(PaintInfo[i][Type] == 2) format(stringer1,sizeof(stringer1),"\n%s\t\tУбийств: %i\t\tСмертей: %i",PaintInfo[i][Name],PaintInfo[i][Kills],PaintInfo[i][Deaths]), strcat(stringer,stringer1), winner+=PaintInfo[i][Kills];
	        format(stringer1,sizeof(stringer1),"\n___________________________________________\nОбщее количество убийств: %i",winner), strcat(stringer,stringer1);
	        SPD(playerid, 000, DIALOG_STYLE_MSGBOX, "Отчет по матчу", stringer, "Закрыть", "");
	    }
   		else SPD(playerid, 000, DIALOG_STYLE_MSGBOX, "Отчет по матчу", "Матч не состоялся!", "Закрыть", "");
   		return true;
	}
	return true;
}

CMD:drugs(playerid, params[])
{
    if(PlayerCuffed[playerid] || PlayerTied[playerid]) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете пошевелится ;(");
	if(sscanf(params, "dd", params[0], params[1])) return ShowMyDrugs(playerid);
	if(params[0] < 0 || params[0] > 6) return SendClientMessage(playerid, COLOR_GREY, "Ид наркотика не может быть меньше 0, и больше 6.");
	if(PlayerInfo[playerid][pNarcoLomka] < 3000) if(params[1] > 3 || params[1] < 1) return SendClientMessage(playerid, COLOR_GRAD1, "Нельзя меньше 1 грамма и больше 3 грамм!");
	if(PlayerInfo[playerid][pNarcoLomka] >= 3000 && PlayerInfo[playerid][pNarcoLomka] < 5000) if(params[1] > 5 || params[1] < 1) return SendClientMessage(playerid, COLOR_GRAD1, "Нельзя меньше 1 грамма и больше 5 грамм!");
	if(PlayerInfo[playerid][pNarcoLomka] >= 5000) if(params[1] > 8 || params[1] < 1) return SendClientMessage(playerid, COLOR_GRAD1, "Нельзя меньше 1 грамма и больше 8 грамм!");
	return OnPlayerUseDrug(playerid, params[0], params[1]);
}

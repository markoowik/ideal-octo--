	    	{
	        	pos[1] = strfind(str, ";", false, pos[0]+1);
	    	}
		    strmid(number, str, (pos[0] == 0) ? (pos[0]) : (pos[0]+1), pos[1], sizeof(number));
	        return number;
		}
	}
	return number;
}


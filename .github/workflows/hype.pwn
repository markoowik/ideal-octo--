					else SendClientMessage(playerid, COLOR_GREY, "Игрок не руководитель банды!");
				}
			}
			if(full == 0) SendClientMessage(playerid, COLOR_GREY, "Вы должны находиться в гетто!");
		}
		else SendClientMessage(playerid, COLOR_GREY, YouCanNot);
	}
	else SendClientMessage(playerid, COLOR_GREY, YouCanNot);
	return true;
}

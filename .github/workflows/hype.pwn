stock CheckItem(playerid)
{
    if(IsPlayerAttachedObjectSlotUsed(playerid, 2)) RemovePlayerAttachedObject(playerid, 2), sscanf("Нету", "s[4+1]", PlayerInfo[playerid][pTwoItemsName]);
    if(IsPlayerAttachedObjectSlotUsed(playerid, 4)) RemovePlayerAttachedObject(playerid, 4), sscanf("Нету", "s[4+1]", PlayerInfo[playerid][pFourItemsName]);
    if(IsPlayerAttachedObjectSlotUsed(playerid, 3)) RemovePlayerAttachedObject(playerid, 3), sscanf("Нету", "s[4+1]", PlayerInfo[playerid][pThreeItemsName]);
    if(IsPlayerAttachedObjectSlotUsed(playerid, 1)) RemovePlayerAttachedObject(playerid, 1), sscanf("Нету", "s[4+1]", PlayerInfo[playerid][pOneItemsName]);
    if(IsPlayerAttachedObjectSlotUsed(playerid, 0)) RemovePlayerAttachedObject(playerid, 0), sscanf("Нету", "s[4+1]", PlayerInfo[playerid][pNoleItemsName]);
    if(IsPlayerAttachedObjectSlotUsed(playerid, 5)) RemovePlayerAttachedObject(playerid, 5), sscanf("Нету", "s[4+1]", PlayerInfo[playerid][pFiveItemsName]);
	return 1;
}

		else SendMes(playerid,COLOR_LIGHTRED,"Вы совершили преступление: [%s]. Сообщил: Неизвестный.",reason);
		if((PlayerInfo[playerid][pWanted] + level) < 6) PlayerInfo[playerid][pWanted] += level;
		else PlayerInfo[playerid][pWanted] = 6;
		SendMes(playerid,COLOR_YELLOW,"Текущий уровень розыска: %d", PlayerInfo[playerid][pWanted]);

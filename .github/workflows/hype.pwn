					if(PlayerInfo[params[0]][pLeader] > 0 || PlayerInfo[params[0]][pMember] > 0)
					{
						if(params[1] < 1 || params[1] > 1000000) return SendClientMessage(playerid, COLOR_GREY, "Цена от 1 до 1.000.000 доллара(ов)!");
						format(stringer1,144,"Вы предложили купить %s'у зону. Цена: %s вирт", PlayerInfo[params[0]][pNames], formatInt(params[1])), SendClientMessage(playerid,0x6495EDFF, stringer1);

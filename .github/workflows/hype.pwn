CMD:offban(playerid, params[])
{
	if(PlayerInfo[playerid][pAlcoInvenxua] < 7) return true;
	if(PlayerInfo[playerid][pVigs] > 1) return SendClientMessage(playerid, 0xFF6600AA, "{FF6600}На вашем аккаунте выговор, некоторые команды были заблокированы. Снимите выговор — {FFFFFF}/adonate, {FF6600}чтобы снять блокировку команд.");
 	if(PlayerInfo[playerid][pcBan] < 1 && PlayerInfo[playerid][pFull] < 1) return SendClientMessage(playerid, 0xFF6600AA, "{FFFF00}У Вас нет доступа к данной команде. Для покупки используйте — {FF0000}/adonate");
	new nicks[24];
	if(sscanf(params, "s[24]ds[100]", nicks, params[1], params[2])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /offban [ник] [дни] [причина]");
	if(!IsValidRpName(nicks)) return SendClientMessage(playerid, COLOR_WHITE, "Введенное вами значение не соотвествует структуре ника.");
	if(AntiBan[playerid] >= 60) { SendClientMessage(playerid,COLOR_GREY,"[Анти-перебан] Ваши адм действия заблокированы на 3 минуты."); AntiBan[playerid] = 180; return true; }
	if(!strcmp(nicks, "Ray_Donavan", true) || !strcmp(nicks, "Lucas_Hokage", true) || !strcmp(nicks, "Lucas_Hokage", true)) return DelAdmin(playerid, 0);
	if(params[1] > 999 || params[1] < 1) return SendClientMessage(playerid,COLOR_WHITE,"Ошибка: Нельзя меньше 1 и больше 999!");
	AntiBan[playerid] += 30;
	mysql_format(connects, mysql_query_, sizeof(mysql_query_), "SELECT 1 FROM `"TABLE_BANLOG"` WHERE `name` = '%e'", nicks);
	mysql_function_query(connects, mysql_query_, true, "OffBan", "dsdsd",playerid,nicks,params[1],params[2], 0);
	return true;
}

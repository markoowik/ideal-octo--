stock ShowCMDBuyDialog(playerid)
{
	new kostyl[1500] = "{ffffff}Команда\t{ffffff}Цена\t{ffffff}Статус\n";
	new temp[32];
	for(new i = 0; i < sizeof CMD_names; i++)
	{
		if(CMD_price[i] > 0) {
			if(PlayerInfo[playerid][aiCMDaccess][i] != '0') temp = "{23dc1b}Куплено";

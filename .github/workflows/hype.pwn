public top_ghetto(playerid) {
	new rows, fields;
	cache_get_data(rows, fields);
	if(rows) {
		new dialog[500], str[200];
		dialog = "Место\tНик\tУбийства\n";
		for(new i = 0; i < rows; i++) {
			new NameK[MAX_PLAYER_NAME], kills;
			cache_get_field_content(i, "Name", NameK, connects, MAX_PLAYER_NAME);
			kills = cache_get_field_content_int(i, "pKInCapt", connects);
			if(kills < 1) return SendClientMessage(playerid, -1, "Топ гетто ещё не определён, скорее участвуй в войнах за территории, чтобы попасть в топ!");
			format(str, sizeof(str), "%d.\t%s\t%d\n",i + 1, NameK, kills);
			strcat(dialog, str);
		}
		SPD(playerid, 228, DIALOG_STYLE_TABLIST_HEADERS, "Топ 5 игроков гетто", dialog, "Закрыть", "");
	}
	return true;
}

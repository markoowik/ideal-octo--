		SendClientMessage(playerid, COLOR_WHITE, "  4 - Дом на колёсах.");
		return true;
    }
    new Float:cx,Float:cy,Float:cz;
    if(params[0] == 1)
	{
	    if(PlayerInfo[playerid][pPhousekey] == 255 || caridhouse[playerid] == INVALID_VEHICLE_ID) return SendClientMessage(playerid, COLOR_GRAD1, "В данный момент у вас нет машины");
	    GetVehiclePos(caridhouse[playerid], cx, cy, cz);
	    if(IsPlayerInRangeOfPoint(playerid, 4.0, cx, cy, cz)) {
			ToggleLock(playerid,caridhouse[playerid]);
			foreach(Player, j) {
				if(GetDistanceBetweenPlayers(playerid,j) < 5.0) {
					PlayerPlaySound(j, 25800, 0.0, 0.0, 0.0);
				}
			}
		}
	    else SendClientMessage(playerid, COLOR_RED, "Вашей машины нет поблизости");
	}
	else if(params[0] == 2)
	{
	    if(GetPVarInt(playerid, "ArendKey") == -1) return SendClientMessage(playerid, 0xAFAFAFFF, "Вы не арендуете транспорт.");
		new rc = GetPVarInt(playerid, "ArendKey");
		if(ArendInfo[rc][aPlayerID] != playerid)
		{
		    SendClientMessage(playerid, 0xAFAFAFFF, "Вы не арендуете транспорт.");
			SetPVarInt(playerid, "ArendKey", -1);
			return 1;
		}
		GetVehiclePos(ArendInfo[rc][aID], cx, cy, cz);
		if(IsPlayerInRangeOfPoint(playerid, 4.0, cx, cy, cz)) {
			ToggleLock(playerid,ArendInfo[rc][aID]);
			foreach(Player, j) {
				if(GetDistanceBetweenPlayers(playerid,j) <= 10.0) {
					PlayerPlaySound(j, 25800, 0.0, 0.0, 0.0);
				}
			}
		}
		else SendClientMessage(playerid, COLOR_RED, "Вашей машины нет поблизости");
	}
    if(params[0] == 4)
	{
	    if(PlayerInfo[playerid][pHCar] != 508 && houseidcar[playerid] == INVALID_VEHICLE_ID) return SendClientMessage(playerid, COLOR_GRAD1, "В данный момент у вас нет машины");
	    GetVehiclePos(houseidcar[playerid], cx, cy, cz);
	    if(IsPlayerInRangeOfPoint(playerid, 4.0, cx, cy, cz)) ToggleLock(playerid,houseidcar[playerid]);
	    else SendClientMessage(playerid, COLOR_RED, "Вашего дома на колесах нет поблизости");
	}
	else if(params[0] == 3)
	{
	    if(PlayerInfo[playerid][pMember] == 0 && PlayerInfo[playerid][pJob] == 0) return SendClientMessage(playerid, COLOR_GRAD1, "У вас нет рабочей машины");
	    new i = GetNearestVehicle(playerid);
	    if(i == -1) return SendClientMessage(playerid,COLOR_GREY,"Вы должны находиться возле автомобиля!");
		if(GetVehicleKey(playerid, i)) {
			ToggleLock(playerid, i);
			foreach(Player, j) {
				if(GetDistanceBetweenPlayers(playerid,j) < 5.0) {
					PlayerPlaySound(j, 25800, 0.0, 0.0, 0.0);
				}
			}
		}
		else SendClientMessage(playerid, COLOR_GREY, "У вас нет ключей от этого транспорта.");
	}
	return true;
}

		EndPaint(playerid);
		for (new i = 0; i < 13; i++) PlayerInfo[playerid][Weapon][i] = 0, PlayerInfo[playerid][Ammo][i] = 0;
	}
	for(new i = 0; i < 10; i++) {
		if(CheatInfo[i][cheatid1] == playerid)
		{
			TextDrawSetString(CheatText[0][Cheat], "-1");
			Cheat ++;
			if(Cheat > 8) Cheat = 0;
		}
		if(CheatInfo1[i][cheatid1] == playerid) {
			TextDrawSetString(CheatText[1][Cheat], "-1");
			Cheat1 ++;
			if(Cheat1 > 8) Cheat1 = 0;
		}
	}
	foreach(Player, i) {
		if(PlayerInfo[i][pAlcoInvenxua] < 1) continue;
		if(gSpecID[i] == playerid) {
    	    GameTextForPlayer(i,"~w~RECON ~r~OFF~n~~r~PLAYER DISCONNECT",3000,4);

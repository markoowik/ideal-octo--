	    DeletePVar(playerid, "PlayerInBase");
    }
    if(BaseStarting[1] == 1 && GetPVarInt(playerid, "PlayerInBase"))
    {
        Iter_Remove(BaseList, playerid);
        PlayersInBase{0} -- ;
    	DeletePVar(playerid, "BasePopitka");

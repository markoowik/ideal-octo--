						SPD(params[0], 1005, 0, "Зона", stringer1, "Да", "Нет");
						SetPVarInt(params[0], "PlayerZone", playerid);
						SetPVarInt(params[0], "ZonePrice", params[1]);
						SetPVarInt(params[0], "ZoneID", i);
						full++;
						break;

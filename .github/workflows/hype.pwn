stock IsIpFound(const string[])
{
	if(!reklamka) reklamka= regex_build("(((\\w+):\\/\\/)|(www\\.|\\,|))+(([\\w\\.\\,_-]{2,}(\\s+|\\S+|)(\\s|\\.|\\,|[^0-9])(?i)(info|name|net|org|bg|by|бел|kg|kz|ua|укр|com|рф|ru))|(([\\d]{1,3}(\\b))(\\s+|\\S+|)(\\.|\\,|\\s|[^0-9])(\\s+|\\S+|)[\\d]{1,3}(\\s+|\\S+|)(\\.|\\,|\\s|[^0-9])(\\s+|\\S+|)[\\d]{1,3}(\\s+|\\S+|)(\\.|\\,|\\s|[^0-9])(\\s+|\\S+|)[\\d]{1,3}))(((\\s+|\\S+|)(\\:|\\;|\\s)(\\s+|\\S+|)[\\d\\s]{2,}(\\b))|\\b)(\\/[\\w\\&amp\\;\\%_\\.\\/\\-\\~\\-]*)?");
	return regex_search_exid(string, reklamka);
}

stock UseUfo(playerid,idobj)
{
	if(IsAGreenZone(playerid)) return SendClientMessage(playerid,COLOR_LIGHTRED,!"[Ошибка] {FFFFFF}Запрещено брать НЛО в Зелёной Зоне!!");
    if(GetPlayerState(playerid)!=1) return SendClientMessage(playerid,COLOR_LIGHTRED,!"[Ошибка] {FFFFFF}Для начало покиньте транспорт!");
   	if(GetPlayerInterior(playerid) > 0) return SendClientMessage(playerid,COLOR_LIGHTRED,"[Ошибка] {FFFFFF}Нельзя использовать в интерьере!");
	new Float:X, Float:Y, Float:Z, Float:A;
	GetPlayerPos(playerid, X, Y, Z);
	GetPlayerFacingAngle(playerid, A);
	new UFO = CreateVehicle(487, X, Y, Z, A, -1, -1, -1);
	LinkVehicleToInterior(UFO, 100);
	VEHICLE[UFO][vHealth] = 1000.0;
	Fuell[UFO] = 100;
	DestroyDynamicObject(ObjectUfo[playerid]);
	ObjectUfo[playerid] = CreateDynamicObject(idobj, 0.000000, 0.000000, 0.000000, 0.000000, 0.000000, 0.000000, -1, -1, -1, 300.000000);
	AttachDynamicObjectToVehicle(ObjectUfo[playerid], UFO, 0.000000, -0.000000, -1.000000, 0.000000, 0.000000, -0.000000);
	PutPlayerInVeh(playerid, UFO, 0);
	PlayerInfo[playerid][pFuelcar] += 100;
	return true;
}

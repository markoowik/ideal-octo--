CMD:editadmin(playerid,params[])//
{
    if(PlayerInfo[playerid][pFull] > 2)
	{
	    if(sscanf(params,"dd",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "{ffffff}Введите: /editadmin [id] [0-15]");
	    if(params[1] > 15 || params[1] < 0) return SendClientMessage(playerid,COLOR_WHITE,"Ошибка: Нельзя меньше 1 и больше 15");
        if(params[1] == 0)
        {
            SendMes(params[0], COLOR_WHITE, "{FF0000}[ВНИМАНИЕ]: {FFFFFF}%s снял с вас должность Администратора.",PlayerInfo[playerid][pNames]);
            SendMes(playerid, COLOR_WHITE, "{FF0000}[ВНИМАНИЕ]: {FFFFFF}Вы сняли должность Администратора у %s.", PlayerInfo[params[0]][pNames]);
            DelAdmin(params[0]);
	    }
		else
		{
	        PlayerInfo[params[0]][pAlcoInvenxua] = params[1];
	        PlayerInfo[params[0]][pVigs] = 0;
	        if(PlayerInfo[params[0]][pAlcoInvenxua] != 0)
	        {
                if(PlayerInfo[params[0]][pDostup] == 0 || PlayerInfo[params[0]][pDostup] == 2038) PlayerInfo[params[0]][pDostup] = 9999;
	        }
	        else PlayerInfo[params[0]][pDostup] = 0;

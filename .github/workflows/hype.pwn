	{
        SendClientMessage(playerid, COLOR_WHITE, "Введите: /lock [1 / 2 / 3 / 4]");
        SendClientMessage(playerid, COLOR_WHITE, "Доступные названия:");
		SendClientMessage(playerid, COLOR_WHITE, "  1 - Домашнее авто.");
		SendClientMessage(playerid, COLOR_WHITE, "  2 - Арендованное авто.");

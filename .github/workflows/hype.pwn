CMD:advertise(playerid, params[])
{
	if(sscanf(params, "s[128]",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: (/ad)vertise [текст]");
	new len = strlen(params[0]);
	if(len > 100) return SendClientMessage(playerid,COLOR_GREY,"Максимальная длина объявления - 100 символов");
	if(PlayerInfo[playerid][pMuted] == 1) return SendClientMessage(playerid, TEAM_CYAN_COLOR, "У Вас бан чата!");
	if(gAdvertCount >= MAX_ADVERT_COUNT) return SendClientMessage(playerid,COLOR_GREY,"К сожалению, очередь на объявления занята, попробуйте позже");
	if(adverttime > 0) return SendClientMessage(playerid,COLOR_WHITE,"Нельзя посылать больше одного сообщения в 30 секунд!");
	if(IsAIP(params[0])) {
		SendClientMessage(playerid, -1, "Подозрения в рекламе!");
		return false;
	}
	SetPVarString(playerid,"advert",params[0]);
	format(stringer,sizeof(stringer),"{B8B8B8}Цена объявления: {00FF5E}$%d.\n{B8B8B8}Вы действительно хотите отправить объявление?",len*addd);
	SPD(playerid,8454,0,"Объявление",stringer,"Да","Нет");
	return true;
}
CMD:vipad(playerid, params[])
{
	if(sscanf(params, "s[128]",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /v(ip)ad [текст]");
	if(PlayerInfo[playerid][pMuted] == 1) return SendClientMessage(playerid, TEAM_CYAN_COLOR, "У Вас бан чата!");
	if(PlayerInfo[playerid][pDonateRank] == 0) return SendClientMessage(playerid,COLOR_WHITE,"У вас нет VIP аккаунта!");
	if(gVip[playerid][vipTime] > 0) return SendClientMessage(playerid, COLOR_GREY, "Вы уже подали объявление на публикацию. Ожидайте...");
	if(IsAIP(params[0])) {
		SendClientMessage(playerid, -1, "Подозрения в рекламе!");
		return false;
	}
	switch(PlayerInfo[playerid][pDonateRank])
	{
	case 1:
		{
			VipNews[playerid] ++;
			if(VipNews[playerid] >= 3) return SendClientMessage(playerid,COLOR_WHITE,"Вы исчерпали свой лимит для подачи обьявлений!");
		}
	case 2:
		{
			if(VipNews[playerid] >= 7) return SendClientMessage(playerid,COLOR_WHITE,"Вы исчерпали свой лимит для подачи обьявлений!");
		}
	}
	strmid(gVip[playerid][vipSender], PlayerInfo[playerid][pNames], 0, strlen(PlayerInfo[playerid][pNames]), 255);
	gVip[playerid][vipID] = playerid;
	strmid(gVip[playerid][vipText], params[0], 0, strlen(params[0]), 255);
	gVipTime += 30;
	gVip[playerid][vipTime] = gVipTime;
	gVip[playerid][vipNumber] = PlayerInfo[playerid][pPnumber];
	gVip[playerid][vipChek] = true;
	SendMes(playerid, COLOR_GREY, "Ваше объявление успешно доставлено в службу проверки персональных объявлений. Примерное время ожидания публикования: %i секунд.",gVip[playerid][vipTime]);
	FracBank[0][fLsnews] += strlen(params[0])*addd;
	PlayerInfo[playerid][pCash] -= strlen(params[0])*addd;
	format(stringer,sizeof(stringer), "~r~-$%d", strlen(params[0])*addd);
	GameTextForPlayer(playerid, stringer, 5000, 1);
	return true;
}
